{
  "name": "@mozu/utils",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/team-mozu/colons.git",
    "directory": "packages/common/utils"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "pnpm clean && tsc -p tsconfig.build.json --emitDeclarationOnly --declarationDir dist && rollup -c rollup.config.js",
    "coverage": "vitest run --coverage",
    "prepack": "pnpm build",
    "test": "vitest run && pnpm typecheck",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@mozu/rollup-config": "workspace:^0.1.0",
    "@mozu/vitest": "workspace:^0.1.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "colons": "file:../../../",
    "rollup": "^4.52.5",
    "typescript": "5.6.3"
  },
  "publishConfig": {
    "access": "public",
    "export": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json"
    },
    "import": "./esm/index.mjs",
    "main": "./dist/index.js",
    "module": "./esm/index.mjs",
    "types": "./dist/index.d.ts"
  }
}
